{
    "name": "ruwler/ruwler-sdk-bundle",
    "homepage": "https://api.ruwler.io",
    "type": "symfony-bundle",
    "description" : "Ruwler SDK Bundle",
    "keywords": ["ruwler-sdk-bundle", "ruwler", "sdk-php", "ruwler-php", "symfony"],
    "license": "Apache-2.0",
    "require": {
        "php": "^7.1",
        "ruwler/ruwler-sdk-php": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35|^5.4.0|^6.0.0",
        "symfony/framework-bundle": "~2.3|~3.0|~4.0",
        "symfony/yaml": "~2.3|~3.0|~4.0"
    },
    "autoload": {
        "psr-4": {
            "Ruwler\\SdkBundle\\": ""
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
